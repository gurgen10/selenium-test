<script>
function addServicebotScript() {
    var script = document.createElement("script");  // create a script DOM node
    script.src = '/app/js/ls-pricing-page.js';  // set its src to the provided URL
    script.async = 'async';

    document.body.appendChild(script);
    window.removeEventListener('scroll', addServicebotScript, false);
} 
window.addEventListener('scroll',addServicebotScript, false);    
</script>

